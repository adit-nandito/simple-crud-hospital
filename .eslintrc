{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": ["jest"],
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": ["off", {"devDependencies": true}],
    "no-underscore-dangle": "off"
  }
}
